{
    "name": "mlatjac/bjrhi",
    "description": "Use on a new Laravel project to setup ui scaffolding including register and login in both French and English",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Legault",
            "email": "miguel.legault@ctrlweb.ca",
            "homepage": "https://ctrlweb.ca"
        }
    ],
    "homepage": "https://github.com/mlatjac/bjrhi",
    "keywords": ["Laravel", "BjrHi"],
    "require": {
        "illuminate/support": "~6",
        "caouecs/laravel-lang": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^4.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mlatjac\\BjrHi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mlatjac\\BjrHi\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mlatjac\\BjrHi\\BjrHiServiceProvider"
            ],
            "aliases": {
                "BjrHi": "Mlatjac\\BjrHi\\Facades\\BjrHi"
            }
        }
    }
}
